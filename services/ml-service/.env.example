PREDICTION_CACHE_TTL=900

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/adaptive_learning

# Monitoring
PROMETHEUS_PORT=8001
LOG_LEVEL=INFO

# Environment
ENVIRONMENT=development