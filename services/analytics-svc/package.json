{
  "name": "@drivemaster/analytics-svc",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch src/server.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts",
    "format": "prettier -w .",
    "test": "vitest run",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx src/seed.ts"
  },
  "dependencies": {
    "@drivemaster/shared-config": "workspace:*",
    "@drivemaster/redis-client": "workspace:*",
    "@drivemaster/kafka-client": "workspace:*",
    "@drivemaster/es-client": "workspace:*",
    "@drivemaster/telemetry": "workspace:*",
    "@fastify/cors": "^8.4.2",
    "@fastify/jwt": "^7.2.4",
    "@prisma/client": "^5.7.1",
    "fastify": "^4.28.1",
    "pino": "^9.5.0",
    "prom-client": "^15.1.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "prisma": "^5.7.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  }
}
